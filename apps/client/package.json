{
    "name": "@sma-v4/client",
    "private": true,
    "version": "4.0.4",
    "type": "module",
    "scripts": {
        "build": "run-s build:development",
        "build:development": "run-s build:vite:development",
        "build:emulator": "run-s build:vite:emulator",
        "build:live": "run-s replaceGoogleServicesJson:live replaceGoogleServicePlist:live replaceFbsw:live build:vite:live",
        "build:vite:development": "tsc; vite build --mode development",
        "build:vite:emulator": "tsc; vite build --mode emulator",
        "build:vite:live": "tsc; vite build --mode live",
        "i18n:compare": "yarn i18n:extract; node scripts/compare_json.cjs temp/raw_en.json src/hooks/I18n/locales/en.json",
        "i18n:compile": "formatjs compile temp/raw_en.json --out-file temp/en.json --format=simple",
        "i18n:extract": "formatjs extract 'src/**/*.tsx*' --ignore '**/*.d.ts' --out-file ./temp/raw_en.json --format formatter.cjs",
        "lint": "eslint",
        "open:development:ios": "run-s replaceGoogleServicePlist:development build:development cap:sync cap:open:ios",
        "open:live:ios": "run-s replaceGoogleServicePlist:live build:live cap:sync cap:open:ios",
        "open:development:android": "run-s replaceGoogleServicesJson:development build:development cap:sync cap:open:android",
        "open:live:android": "run-s replaceGoogleServicesJson:live build:live cap:sync cap:open:android",
        "preview": "vite preview",
        "replaceFbsw:development": "cp ./fbsw/firebase-messaging-sw.development.js ./public/firebase-messaging-sw.js",
        "replaceFbsw:emulator": "cp ./fbsw/firebase-messaging-sw.emulator.js ./public/firebase-messaging-sw.js",
        "replaceFbsw:live": "cp ./fbsw/firebase-messaging-sw.live.js ./public/firebase-messaging-sw.js",
        "replaceGoogleServicePlist:development": "cp ./google_services/GoogleService-Info_development.plist ./ios/App/App/GoogleService-Info.plist",
        "replaceGoogleServicePlist:live": "cp ./google_services/GoogleService-Info_live.plist ./ios/App/App/GoogleService-Info.plist",
        "replaceGoogleServicesJson:development": "cp ./google_services/google-services_development.json ./android/app/google-services.json",
        "replaceGoogleServicesJson:live": "cp ./google_services/google-services_live.json ./android/app/google-services.json",
        "serve:vite:development": "vite --mode development",
        "serve:vite:emulator": "vite --mode emulator",
        "serve:vite:live": "vite --mode live",
        "serve": "run-s replaceFbsw:development serve:vite:development",
        "serve:development": "run-s replaceFbsw:development serve:vite:development",
        "serve:emulator": "run-s replaceFbsw:emulator serve:vite:emulator",
        "serve:live": "run-s replaceFbsw:live serve:vite:live",
        "cap:sync": "cap sync",
        "cap:open:android": "cap open android",
        "cap:open:ios": "cap open ios",
        "storybook": "storybook dev -p 6006",
        "test.e2e": "cypress run",
        "test.e2e:open": "cypress open",
        "test.unit": "vitest",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@capacitor-firebase/messaging": "^5.4.1",
        "@capacitor/android": "^6.0.0",
        "@capacitor/app": "5.0.7",
        "@capacitor/app-launcher": "^5.0.7",
        "@capacitor/core": "5.7.0",
        "@capacitor/device": "^5.0.7",
        "@capacitor/geolocation": "^5.0.7",
        "@capacitor/haptics": "5.0.7",
        "@capacitor/ios": "^5.7.0",
        "@capacitor/keyboard": "5.0.8",
        "@capacitor/preferences": "^5.0.7",
        "@capacitor/push-notifications": "^5.1.1",
        "@capacitor/status-bar": "5.0.7",
        "@hookform/error-message": "^2.0.1",
        "@hookform/resolvers": "^3.3.4",
        "@ionic/react": "^7.0.0",
        "@ionic/react-router": "^7.0.0",
        "@material-design-icons/svg": "^0.14.13",
        "@material-symbols/svg-400": "^0.16.0",
        "@material-symbols/svg-700": "^0.18.0",
        "@share-meals/frg-ui": "^0.0.17",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "classnames": "^2.5.1",
        "date-fns": "^3.6.0",
        "firebase": "^10.8.0",
        "ionicons": "^7.0.0",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-geocode": "^1.0.0-alpha.1",
        "react-hook-form": "^7.50.1",
        "react-intl": "^6.6.2",
        "react-joyride": "^2.8.0",
        "react-markdown": "^9.0.1",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4",
        "react-toastify": "^10.0.5",
        "suspend-react": "^0.1.3",
        "swr": "^2.2.5"
    },
    "devDependencies": {
        "@capacitor/assets": "^3.0.4",
        "@capacitor/cli": "5.7.0",
        "@chromatic-com/storybook": "^1.2.23",
        "@formatjs/cli": "^6.2.7",
        "@storybook/addon-a11y": "^8.0.2",
        "@storybook/addon-actions": "^8.0.4",
        "@storybook/addon-console": "^3.0.0",
        "@storybook/addon-essentials": "^8.0.2",
        "@storybook/addon-interactions": "^8.0.2",
        "@storybook/addon-links": "^8.0.2",
        "@storybook/addon-onboarding": "^8.0.2",
        "@storybook/addon-viewport": "^8.0.2",
        "@storybook/blocks": "^8.0.2",
        "@storybook/react": "^7.6.17",
        "@storybook/react-vite": "^8.0.2",
        "@storybook/test": "^8.0.2",
        "@testing-library/dom": ">=7.21.4",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-legacy": "^5.0.0",
        "@vitejs/plugin-react": "^4.0.1",
        "cypress": "^13.5.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.35.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-storybook": "^0.8.0",
        "jsdom": "^22.1.0",
        "npm-run-all": "^4.1.5",
        "react-lorem-ipsum": "^1.4.10",
        "storybook": "^8.0.2",
        "storybook-dark-mode": "^4.0.1",
        "terser": "^5.4.0",
        "typescript": "^5.1.6",
        "vite": "^5.0.0",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^0.34.6"
    },
    "description": "An Ionic project"
}
